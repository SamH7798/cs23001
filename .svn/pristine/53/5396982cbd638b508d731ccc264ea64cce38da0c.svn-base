// bigint Test Program
//
// Tests:  default constructor, uses ==
//
//  
#include <iostream>
#include <cassert>
#include "bigint.hpp"

//===========================================================================
int main () {
    // Setup

    // Test 
    bigint bi;
    bigint bi2(0);
    // Verify
    std::cout << "0 == " << bi << std::endl;
    assert(bi == bi2); std::cout <<"made it\n";
    assert(bi == 0);

    std::cout << "Done testing default constructor." << std::endl;
}
