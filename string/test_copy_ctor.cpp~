#include "string.hpp"
#include <cassert>
#include <iostream>

//===========================================================================
int main ()
{
  {
    //------------------------------------------------------
    // SETUP FIXTURE

    // TEST
    String str1("helloworld");
    String str2 = str1;
    // VERIFY
    assert(str2 == "helloworld");
  }

  std::cout<< "Done testing copy constructor" << std::endl;




}
