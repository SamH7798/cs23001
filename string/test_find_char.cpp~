#include "string.hpp"
#include <cassert>
#include <iostream>

//===========================================================================
int main ()
{
  {
    //------------------------------------------------------
    // SETUP FIXTURE
    String  str1("abcde");
     
   
    // VERIFY
    
    assert(str1.findchar(0, 'b') == 1);
  }
   
     

  {
    //------------------------------------------------------
    // SETUP FIXTURE
    String  str1("hello");


    // VERIFY

    assert(str1.findchar(0, 'h') == 0);
  }



  


  std::cout<< "Done testing find character" <<std::endl;

    
}
